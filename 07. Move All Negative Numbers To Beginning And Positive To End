#include <bits/stdc++.h> 
vector<int> separateNegativeAndPositive(vector<int> &nums){
    // Write your code here.
    int n=nums.size();
    for(int i=0;i<n;i++)
    {
       for(int j=i+1;j<n;j++)
       {

          if(nums[i]>nums[j])
           {
               swap(nums[i],nums[j]);
               i++;
               j++;
           }

           if(nums[i]>0)
           {
               i++;
           }
           else{
               j++;
           }
       }
    }
   
}
